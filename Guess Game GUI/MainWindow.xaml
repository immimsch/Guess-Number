<Window x:Name="GuessGameGUI" x:Class="Guess_Game_GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Guess_Game_GUI"
        mc:Ignorable="d"
        Title="Guess Game" Height="330" Width="400">
    <Grid>
        <Label x:Name="DigitLabel" Content="Please Select The Digit Of Guess Number." HorizontalAlignment="Left" Height="50" Margin="20,20,0,0" VerticalAlignment="Top" Width="360" FontSize="18"/>
        <Button x:Name="ThreeButton" Content="3" HorizontalAlignment="Left" Height="50" Margin="60,100,0,0" VerticalAlignment="Top" Width="80" FontSize="20" Click="ThreeButton_Click"/>
        <Button x:Name="FourButton" Content="4" HorizontalAlignment="Left" Height="50" Margin="260,100,0,0" VerticalAlignment="Top" Width="80" FontSize="20" Click="FourButton_Click"/>
        <Button x:Name="FiveButton" Content="5" HorizontalAlignment="Left" Height="50" Margin="60,200,0,0" VerticalAlignment="Top" Width="80" FontSize="20" Click="FiveButton_Click"/>
        <Button x:Name="SixButton" Content="6" HorizontalAlignment="Left" Height="50" Margin="260,200,0,0" VerticalAlignment="Top" Width="80" FontSize="20" Click="SixButton_Click"/>

    </Grid>
</Window>
